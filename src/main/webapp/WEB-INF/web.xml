<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" 
		version="3.0">

<!-- DEPLOYMENT DESCRIPTOR FOR CLASS SCHEDULE PROGRAM -->

	<!-- Name to display for the application -->
	<display-name>Class Schedule</display-name>
	
	<!-- Display these pages, in order of first found, when this application is accessed -->
	<welcome-file-list>
